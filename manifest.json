{
  "manifest_version": 2,
  "name": "Trader",
  "version": "1.0",
  
  "icons": {
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": [ "/js/content.js" ]
    }
  ],
  "background": {
    "scripts": ["/js/background.js"]
  },
  "permissions": ["activeTab","webRequest","webRequestBlocking","*://*/*"],

      "browser_action": {
        "default_title": "Trader",

        "default_popup": "popup.html"
    }
	
}